      "source": [
        "def char_frequency(c):\n",
        "    a = {}\n",
        "    for n in c:\n",
        "        b = a.keys()\n",
        "        if n in b:\n",
        "            a[n] += 1\n",
        "        else:\n",
        "            a[n] = 1\n",
        "    return a\n",
        "char_frequency(\"hippo runs to us!\")"
      ],
